
<!DOCTYPE html>
<html lang="pt-BR">
<head>
  <meta charset="utf-8">
  <title>Relatório de Faturamento</title>
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Arimo:ital,wght@0,400..700;1,400..700&display=swap" rel="stylesheet">
  <style>
    html, body {
      margin: 0;
      padding: 0;
      font-family: "Arimo", Helvetica, sans-serif;
      font-optical-sizing: auto;
      font-weight: 400;
      font-style: normal;
      font-size: 13px;
      color: #000;
    }

    @page {
      size: A4;
      margin: 10mm 10mm 15mm 10mm;
    }

    .header {
      width: 100%;
      border-bottom: 1px solid #000;
      margin-bottom: 8px;
      table-layout: auto;
    }

    .header td {
      vertical-align: top;
    }

    .company-name {
      font-weight: bold;
      font-size: 15px;
    }

    .small {
      font-size: 12px;
    }

    .right {
      text-align: right;
    }

    .center {
      text-align: center;
    }

    .left {
      text-align: left;
    }

    .bold {
        font-weight: bold;
    }

    .box {
      border: none;
      padding: 4px;
      margin-bottom: 6px;
    }

    table {
      width: 100%;
      border-collapse: collapse;
    }

    th {
      border-left: none;
      border-right: none;
      border-top: 1px solid #000;
      border-bottom: 1px solid #000;
      padding: 4px;
      font-size: 12px;
    }

    td {
      border: 1px solid #000;
      padding: 4px;
      font-size: 12px;
      border: none;
    }

    .no-border td {
      border: none;
    }

    .totals {
        border-top: 1px solid #000;
        border-bottom: 1px solid #000;
        table-layout: auto;
    }

    .totals td {
      font-weight: bold;
      font-size: 13px;
      padding: 4px 16px
    }

    .totals td:first-child {
      padding-left: 128px
    }

    .totals td:last-child {
      padding-right: 128px
    }

    .items td {
        padding: 6px 8px
    }

    .td-min-width {
      width: 1%;
    }

    .nowrap {
      white-space: nowrap;
      text-wrap: nowrap;
    }

  </style>
</head>

<body>
  <% const e = (typeof cabecalho_emp !== 'undefined' && cabecalho_emp) ? cabecalho_emp : {}; %>
  <% const c = (typeof cabecalho !== 'undefined' && cabecalho) ? cabecalho : {}; %>
  <% const p = (typeof pedidos !== 'undefined' && pedidos) ? pedidos : []; %>
  <% const t = (typeof totais !== 'undefined' && totais) ? totais : {}; %>

  <!-- HEADER -->
  <table class="header">
    <tr>
      <td style="width: 1%;">
          <img style="width: 128px; height: auto;" src="data:image/png;base64," />
      </td>
      <td>
          <table>
              <tr>
                  <td><div class="company-name"><%= e.NOME_EMP %></div></td>
              </tr>
              <tr>
                  <td><span class="bold">Endereço: </span><%= e.ENDERECO_EMP %></td>
                  <td><span class="bold">Número: </span><%= e.NUMERO_EMP %></td>
              </tr>
              <tr>
                  <td><span class="bold">Bairro: </span><%= e.BAIRRO_EMP %></td>
                  <td><span class="bold">Cidade: </span><%= e.CIDADE_EMP %></td>
              </tr>
              <tr>
                  <td><span class="bold">Estado: </span><%= e.ESTADO_EMP %></td>
                  <td><span class="bold">CEP: </span><%= e.CEP_PEMP %></td>
                  <td><span class="bold">Telefone: </span><%= e.TELEFONE_EMP %></td>
              </tr>
          </table>
      </td>

      <td class="right small" style="width: 1%; white-space: nowrap;">
          <table>
              <tr>
                  <td class="left">Página:</td>
                  <td class="right">1 / 1</td>
              </tr>
              <tr>
                  <td class="left">Data:</td>
                  <td class="right"><%= utils.dateFormatBR(e.DATA_RELATORIO) %></td>
              </tr>
          </table>
      </td>
    </tr>
  </table>

  <!-- CLIENT -->
  <div class="box">
    <table class="no-border">
      <tr>
        <td><strong>Cliente:</strong> <%= c.CLIENTE %></td>
      </tr>
      <tr>
        <td><strong>Endereço:</strong> <%= c.ENDERECO %></td>
        <td><strong>Número:</strong> <%= c.NUMERO %></td>
        <td><strong>CEP:</strong> <%= c.CEP %></td>
      </tr>
      <tr>
          <td><strong>Bairro:</strong> <%= c.BAIRRO %></td>
        <td><strong>Cidade:</strong> <%= c.CIDADE %></td>
        <td><strong>UF:</strong> <%= c.ESTADO %></td>
      </tr>
      <tr>
          <td><strong>Telefone:</strong> <%= c.TELEFONE %></td>
          <td><strong>Complemento:</strong> <%= c.COMPLEMENTO %></td>
      </tr>
    </table>
  </div>

  <!-- SALES TABLE -->
  <table class="items nowrap">
    <thead>
      <tr>
        <th>Pedido</th>
        <th>Espécie</th>
        <th>Data</th>
        <th>Valor</th>
        <th>N° N.F.</th>
        <th>Observações</th>
        <th>Empresa</th>
      </tr>
    </thead>
    <tbody>
      <% (p || []).forEach(item => { %>
        <tr>
          <td class="right" style="width: 8%;"><%= item.PEDIDO %></td>
          <td class="left" style="width: 8%;"><%= utils.trim(item.ESPECIE) %></td>
          <td class="center td-min-width"><%= utils.dateFormatBR(item.DATA) %></td>
          <td class="right" style="width: 16.5%;"><%= utils.moneyFormat(item.VALOR) %></td>
          <td class="right td-min-width">--</td>
          <td class="left"><%= item.OBSERVACOES %></td>
          <td class="right td-min-width"><%= item.EMPRESA %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>

  <br>

  <!-- TOTALS -->
  <table class="totals">
    <tr>
      <td>Total de Vendas</td>
      <td class="right"><%= utils.moneyFormat(t.VALOR_VENDAS) %></td>
      <td class="td-min-width nowrap">Nº de Vendas</td>
      <td class="right td-min-width nowrap"><%= t.TOTAL_VENDAS %></td>
      <td>Crédito Utilizado</td>
      <td class="right"><%= utils.moneyFormat(t.CREDITO) %></td>
    </tr>
    <tr>
      <td>Outras Despesas</td>
      <td class="right"><%= utils.moneyFormat(t.OUTRAS_DESPESAS) %></td>
      <td class="td-min-width"></td>
      <td class="td-min-width"></td>
      <td>Valor Total</td>
      <td class="right"><%= utils.moneyFormat(t.VALOR_TOTAL) %></td>
    </tr>
  </table>

  </body>
</html>
